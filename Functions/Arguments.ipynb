    "# Write a function sum_all that accepts any number of positional arguments and returns their sum.\n",
    "\n",
    "def sumall(*args):\n",
    "    return sum(args)\n",
    "\n",
    "print(sumall(1,2))\n",
    "print(sumall(1,2, 3))\n",
    "print(sumall(1,2, 3, 4))\n",
    "print(sumall(1,2, 3, 4, 5))"
